{
  "name": "paradigm-minimal",
  "version": "1.0.0",
  "description": "Minimal optimized paper trading system",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "auto-trade": "ts-node src/index.ts -- --auto-trade",
    "live-trade": "ts-node src/index.ts -- --live",
    "db:studio": "npx prisma studio --port 5556",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma db push --force-reset",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "kiteconnect": "^5.1.0",
    "open": "^10.2.0",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": [
    "trading",
    "paper-trading",
    "algorithmic-trading"
  ],
  "author": "Paradigm Trading",
  "license": "MIT"
}
